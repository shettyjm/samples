{
  "apiVersion" : "v1",
  "kind" : "List",
  "items" : [ {
    "apiVersion" : "v1",
    "kind" : "Service",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/spring-boot.svg",
        "prometheus.io/port" : "9779",
        "prometheus.io/scrape" : "true"
      },
      "labels" : {
        "expose" : "true",
        "app" : "my-app",
        "provider" : "fabric8",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "com.example"
      },
      "name" : "my-app"
    },
    "spec" : {
      "ports" : [ {
        "name" : "http",
        "port" : 8080,
        "protocol" : "TCP",
        "targetPort" : 8080
      } ],
      "selector" : {
        "app" : "my-app",
        "provider" : "fabric8",
        "group" : "com.example"
      }
    }
  }, {
    "apiVersion" : "extensions/v1beta1",
    "kind" : "Deployment",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/spring-boot.svg",
        "fabric8.io/metrics-path" : "dashboard/file/kubernetes-pods.json/?var-project=my-app&var-version=1.0.0-SNAPSHOT"
      },
      "labels" : {
        "app" : "my-app",
        "provider" : "fabric8",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "com.example"
      },
      "name" : "my-app"
    },
    "spec" : {
      "replicas" : 1,
      "revisionHistoryLimit" : 2,
      "selector" : {
        "matchLabels" : {
          "app" : "my-app",
          "provider" : "fabric8",
          "group" : "com.example"
        }
      },
      "template" : {
        "metadata" : {
          "annotations" : {
            "fabric8.io/iconUrl" : "img/icons/spring-boot.svg",
            "fabric8.io/metrics-path" : "dashboard/file/kubernetes-pods.json/?var-project=my-app&var-version=1.0.0-SNAPSHOT"
          },
          "labels" : {
            "app" : "my-app",
            "provider" : "fabric8",
            "version" : "1.0.0-SNAPSHOT",
            "group" : "com.example"
          }
        },
        "spec" : {
          "containers" : [ {
            "env" : [ {
              "name" : "KUBERNETES_NAMESPACE",
              "valueFrom" : {
                "fieldRef" : {
                  "fieldPath" : "metadata.namespace"
                }
              }
            } ],
            "image" : "my-app:latest",
            "imagePullPolicy" : "IfNotPresent",
            "livenessProbe" : {
              "httpGet" : {
                "path" : "/actuator/health",
                "port" : 8080,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 180
            },
            "name" : "spring-boot",
            "ports" : [ {
              "containerPort" : 8080,
              "name" : "http",
              "protocol" : "TCP"
            }, {
              "containerPort" : 9779,
              "name" : "prometheus",
              "protocol" : "TCP"
            }, {
              "containerPort" : 8778,
              "name" : "jolokia",
              "protocol" : "TCP"
            } ],
            "readinessProbe" : {
              "httpGet" : {
                "path" : "/actuator/health",
                "port" : 8080,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 10
            },
            "securityContext" : {
              "privileged" : false
            }
          } ]
        }
      }
    }
  } ]
}